  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    
    <job>
      <name>heatbeat</name>
      <group>pps</group>
      <description>heatbeat</description>
      <job-type>Aoto.PPS.Core.Jobs.HeartbeatJob, Aoto.PPS.Core</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>postServer</name>
      <group>pps</group>
      <description>postServer</description>
      <job-type>Aoto.PPS.Core.Jobs.PostServerJob, Aoto.PPS.Core</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    
