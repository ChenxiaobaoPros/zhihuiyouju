      <component name="voicePlayer" instance-scope="single-instance" type="Aoto.PPS.Peripheral.Default.VoicePlayer, Aoto.PPS.Peripheral" service="Aoto.PPS.Peripheral.IVoicePlayer, Aoto.PPS.Peripheral">
        <parameters>
          <parameter name="dll" value="peripheral\aoto\AOTO_VOIC_2014.dll" />
