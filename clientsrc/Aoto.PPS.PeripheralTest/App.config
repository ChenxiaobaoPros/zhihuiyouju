      <component name="barScreen" instance-scope="single-instance" type="Aoto.PPS.Peripheral.Default.BarScreen, Aoto.PPS.Peripheral" service="Aoto.PPS.Peripheral.IWriter, Aoto.PPS.Peripheral">
        <parameters>
          <parameter name="dll" value="peripheral\agree\AOTO_BERR_2014.dll" />
