      <component name="compScreen" instance-scope="single-instance" type="Aoto.PPS.Peripheral.Default.CompScreen, Aoto.PPS.Peripheral" service="Aoto.PPS.Peripheral.IWriter, Aoto.PPS.Peripheral">
        <parameters>
          <parameter name="dll" value="peripheral\agree\AOTO_COMP_2014.dll" />
