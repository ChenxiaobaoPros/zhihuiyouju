      <component name="peripheralManager" instance-scope="single-instance" type="Aoto.PPS.Peripheral.PeripheralManager, Aoto.PPS.Peripheral" service="Aoto.PPS.Peripheral.PeripheralManager, Aoto.PPS.Peripheral" />
      <component name="scriptInvoker" instance-scope="single-instance" type="Aoto.PPS.PeripheralTest.FrmShell, Aoto.PPS.PeripheralTest" service="Aoto.PPS.Infrastructure.ComponentModel.IScriptInvoker, Aoto.PPS.Infrastructure"/>

    </components>
